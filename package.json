{
  "name": "react-admin-template",
  "version": "0.1.33",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "prepare": "husky install",
    "commit": "cz",
    "release:msg": "node release-with-msg.js",
    "release:push": "npm run release:msg && git push && git push --tags",
    "release:minor:push": "npm run release:msg minor && git push && git push --tags",
    "release:major:push": "npm run release:msg major && git push && git push --tags"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "axios": "^1.11.0",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "sweetalert2": "^11.23.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "19.1.12",
    "commitizen": "^4.3.1",
    "cz-customizable": "^7.5.1",
    "husky": "^9.1.7",
    "standard-version": "^9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
